<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
    "http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
	<package name="product" extends="default" namespace="/product">
		<action name="viewProduct" class="com.ydev.action.customer.product.ViewProduct">
			<result name="success" type="dispatcher">/customer/product/viewProduct.jsp</result>
		</action>
		
		<action name="viewSpecial" class="com.ydev.action.customer.product.ViewSpecial">
			<result name="success" type="dispatcher">/customer/product/viewSpecial.jsp</result>
		</action>
		
		<action name="viewProductDetail" class="com.ydev.action.customer.product.ViewProductDetail">
			<result name="success" type="dispatcher">/customer/product/viewProductDetail.jsp</result>
		</action>
	</package>
</struts>